{
  "name": "@cloak-js/validator",
  "version": "1.2.0",
  "author": "Gustavo Gentil <gustavorgentil@outlook.com>",
  "license": "ISC",
  "types": "./dist",
  "scripts": {
    "commit": "cz",
    "build": "babel src --out-dir dist --extensions \".ts\" --no-copy-ignored --delete-dir-on-start && npm run ts:declare",
    "ts:declare": "tsc --project tsconfig.declare.json --declaration --declarationMap --emitDeclarationOnly",
    "lint": "eslint src/**/*",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:staged": "npm run test -- --findRelatedTests",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-minify": "^0.5.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-minify": "^0.5.1",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "faker": "^5.0.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.13",
    "semantic-release": "^17.1.1",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "validator": "^13.1.1"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cloak-js/validator.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
